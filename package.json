{
  "name": "11ty-site",
  "version": "1.0.0",
  "description": "a demosite for pcd",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rf _site && mkdir _site && postcss src/static/css/style.css > _site/style.css && eleventy --serve",
    "build": "rm -rf _site && env NODE_ENV=production npx eleventy --pathprefix 'pcd-demosite' && NODE_ENV=production postcss src/static/css/style.css > _site/style.css",
    "start": "rm -rf _site && eleventy && NODE_ENV=production postcss src/static/css/style.css > _site/style.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.7"
  }
}
